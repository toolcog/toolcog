{
  "name": "toolcog",
  "version": "0.0.1",
  "description": "AI native compiler",
  "type": "module",
  "types": "./dist/lib.d.ts",
  "main": "./dist/lib.js",
  "bin": {
    "toolcog": "./cli/dist/bin.js"
  },
  "scripts": {
    "dev": "rollup --config --watch --no-watch.clearScreen",
    "build": "rollup --config",
    "check": "tsc --noEmit",
    "lint": "eslint src/",
    "format": "prettier --check src/",
    "test": "vitest run --passWithNoTests",
    "coverage": "vitest run --coverage",
    "clean": "rimraf .turbo dist",
    "prepack": "toolcog-config prepack",
    "postpack": "toolcog-config postpack"
  },
  "files": [
    "dist/",
    "cli/dist/"
  ],
  "sideEffects": false,
  "imports": {
    "#cli": {
      "types": "./cli/dist/lib.d.ts",
      "import": "./cli/dist/lib.js",
      "require": "./cli/dist/lib.cjs"
    }
  },
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.js",
      "require": "./dist/lib.cjs"
    },
    "./cli": {
      "types": "./cli/dist/lib.d.ts",
      "import": "./cli/dist/lib.js",
      "require": "./cli/dist/lib.cjs"
    },
    "./cli/bin": {
      "types": "./cli/dist/bin.d.ts",
      "import": "./cli/dist/bin.js",
      "require": "./cli/dist/bin.cjs"
    }
  },
  "peerDependencies": {
    "typescript": "^5.5"
  },
  "dependencies": {
    "@toolcog/core": "workspace:*",
    "@toolcog/runtime": "workspace:*",
    "@toolcog/transformer": "workspace:*",
    "commander": "^12.1"
  },
  "optionalDependencies": {
    "@toolcog/openai": "workspace:*"
  },
  "devDependencies": {
    "@toolcog/config": "workspace:*",
    "eslint": "^9.6",
    "prettier": "^3.3",
    "rimraf": "^5.0",
    "rollup": "^4.18",
    "toolcog/cli": "workspace:*",
    "tslib": "^2.6",
    "typescript": "^5.5"
  }
}
